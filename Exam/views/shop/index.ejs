                            <div class="shop-image position-relative overflow-hidden rounded shadow">
                                <a href="#"><img src="<%= product.imageUrl %> " alt="<%= product.title %>" height="400px" width="300px"></a>
                                <ul class="list-unstyled shop-icons">
                                    <li><a href="#" class="btn btn-icon btn-pills btn-soft-danger"><i data-feather="heart" class="icons"></i></a></li>
                                    <li class="mt-2"><a href="#" data-bs-toggle="modal" data-bs-target="#productview" class="btn btn-icon btn-pills btn-soft-primary"><i data-feather="eye" class="icons"></i></a></li>
                                    <li class="mt-2">
                                        <form action="/shopping-cart" method="post">
                                            <input type="hidden" name="productId" value="<%=product._id%>">
                                            <!-- <a href="/shopping-cart" class="btn btn-icon btn-pills btn-soft-warning"><i data-feather="shopping-cart" class="icons"></i></a> -->
                                            <button class="btn btn-primary " type="submit" style="width: 150px;"><i data-feather="shopping-cart" class="icons"></i></button>
                                        </form>
                                    </li
                                </ul>
                            </div>
                            <div class="card-body content pt-4 p-2">
                                <a href="#" class="text-dark product-name h6"><%= product.title %></a>
                                <div class="d-flex justify-content-between mt-1">
                                    <h6 class="text-muted small fst-italic mb-0 mt-1">$<%= product.price %><del class="text-danger ms-2">$<%= product.price + 5.00 %></del> </h6>
                                    <ul class="list-unstyled text-warning mb-0">
                                        <li class="list-inline-item"><i class="fa-solid fa-star"></i></li>
                                        <li class="list-inline-item"><i class="fa-solid fa-star"></i></li>
